<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.brandonlagasse.scheduler2.controller.CustomerAddView">
   <children>
      <Button layoutX="201.0" layoutY="532.0" mnemonicParsing="false" onAction="#saveButton" text="Save" />
      <Button layoutX="64.0" layoutY="532.0" mnemonicParsing="false" onAction="#onCancel" text="Go Back" />
      <TextField fx:id="nameField" layoutX="326.0" layoutY="154.0" promptText="Name" />
      <TextField fx:id="addressField" layoutX="325.0" layoutY="213.0" promptText="Address" />
      <TextField fx:id="postalCodeField" layoutX="325.0" layoutY="275.0" promptText="Postal Code" />
      <TextField fx:id="phoneField" layoutX="325.0" layoutY="328.0" promptText="Phone" />
      <ComboBox fx:id="countryCombo" layoutX="326.0" layoutY="379.0" onAction="#onCountryCombo" prefWidth="150.0" promptText="Country" />
      <ComboBox fx:id="fldCombo" layoutX="326.0" layoutY="413.0" onAction="#onFldCombo" prefWidth="150.0" promptText="First Level Division" />
   </children>
</AnchorPane>
