<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.brandonlagasse.scheduler2.controller.CustomerView">
   <children>
      <TableView fx:id="customerTableView" layoutY="314.0" prefHeight="208.0" prefWidth="800.0">
        <columns>
          <TableColumn fx:id="idCol" prefWidth="75.0" text="ID" />
          <TableColumn fx:id="nameCol" prefWidth="75.0" text="Name" />
            <TableColumn fx:id="addressCol" prefWidth="75.0" text="Address" />
            <TableColumn fx:id="postalCodeCol" prefWidth="75.0" text="Postal Code" />
            <TableColumn fx:id="phoneCol" prefWidth="75.0" text="Phone" />
            <TableColumn fx:id="divisionIdCol" prefWidth="75.0" text="Division ID" />
            <TableColumn fx:id="divisionNameCol" prefWidth="101.0" text="Division Name" />
        </columns>
      </TableView>
      <Button layoutX="23.0" layoutY="543.0" mnemonicParsing="false" onAction="#onExit" text="Go Back" />
      <Button layoutX="484.0" layoutY="543.0" mnemonicParsing="false" onAction="#onAddNewCustomer" text="Add New" />
      <Button layoutX="588.0" layoutY="543.0" mnemonicParsing="false" onAction="#onUpdateCustomer" text="Update" />
      <Button layoutX="675.0" layoutY="543.0" mnemonicParsing="false" onAction="#onDeleteCustomer" text="Delete" />
      <Label layoutX="344.0" layoutY="24.0" text="Customers">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Label layoutX="169.0" layoutY="133.0" text="Search Customers by Name" />
      <TextField fx:id="searchField" layoutX="326.0" layoutY="129.0" onKeyReleased="#onSearchCustomer" />
   </children>
</AnchorPane>
